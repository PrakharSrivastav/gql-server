type Artist {
    id: ID!
    name: String
    # added
    albums: [Album!]
    tracks: [Track!]
}

type Album {
    id: ID!
    name: String
    artistId: ID!
    # Added
    artists:[Artist!]
    tracks:[Track!]
}

type Track {
    id: ID!
    name: String
    artistId: ID!
    albumId: ID!
    # Added
    artists:[Artist!]
    album: Album
}

type Query {
    artist: [Artist!]
    album: [Album!]
    track: [Track!]
}

